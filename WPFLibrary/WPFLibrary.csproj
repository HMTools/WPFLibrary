<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <PackageId>HMTools.WPFLibrary</PackageId>
    <Version>1.1.0.4</Version>
    <Authors>HMTools</Authors>
    <Company>HMTools</Company>
    <Description>Library for common WPF scenarios</Description>
    <Copyright>HMTools</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/HMTools/WPFLibrary</PackageProjectUrl>
    <RepositoryUrl>https://github.com/HMTools/WPFLibrary</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.31" />
    <PackageReference Include="System.Drawing.Common" Version="5.0.0" />
  </ItemGroup>

</Project>
